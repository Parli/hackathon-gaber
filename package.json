    "node": ">=23.6.0"
  },
  "volta": {
    "node": "23.11.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@lustre/sdk": "^3.23.0",
    "dotenv": "^16.5.0",
    "eventemitter3": "^5.0.1"
  }
}
