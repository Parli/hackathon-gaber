{
  "name": "vetted-nest",
  "description": "Vetted Nest - AI-powered home design studio with product recommendations",
  "version": "0.0.0",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm server.ts",
